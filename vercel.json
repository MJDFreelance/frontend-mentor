{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "routes": [
    { "src": "/storybook/(.*)", "dest": "/storybook-static/$1" }
  ],
  "buildCommand": "npm run build && npm run build-storybook",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "./storybook-static"
}
